class Solution(object):
    def isAnagram(self, s, t):
        
        s_list = []
        t_list = []

        for letter in s:
            s_list.append(letter)
        for letter in t:
            t_list.append(letter)

        s_list.sort()
        t_list.sort()

        if s_list == t_list:
            return True
        else:
            return False


