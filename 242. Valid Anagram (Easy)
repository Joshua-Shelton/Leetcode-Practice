class Solution(object):
    def isAnagram(self, s, t):
        
        list1 = []
        list2 = []
        
        for i in s:
            list1.append(i)
        for j in t:
            list2.append(j)
            
        list1.sort()
        list2.sort()
        
        if list1 == list2:
            return True
        return False


